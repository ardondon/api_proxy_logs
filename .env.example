# 服务配置
PORT=3000
NODE_ENV=development

# MySQL数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=api_proxy_logs

# 目标API配置（完整的基础URL，不包含路径）
# 示例: https://api.example.com
# 客户端请求 http://localhost:3000/users 会被转发到 https://api.example.com/users
TARGET_API_URL=https://api.example.com

# 日志配置
LOG_LEVEL=info
LOG_DIR=logs

# 管理后台配置
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123
